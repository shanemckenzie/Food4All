  pod 'Firebase/Auth'

  target 'Food4AllTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Firebase/Core'
  end

  target 'Food4AllUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Firebase/Core'
  end

end
